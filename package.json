{
  "name": "algorithmia-dev-center",
  "version": "1.0.0",
  "private": true,
  "description": "The Algorithmia Developer Resource Center",
  "author": "Algorithmia <mia@algorithmia.com>",
  "homepage": "https://algorithmia.com/",
  "license": "The MIT License (MIT)",
  "scripts": {
    "apidocs:build": "cd api-docs && bundle install && bundle exec rake build",
    "apidocs:setup": "git submodule init && git submodule update && cd api-docs && rvm install 2.3.0 && gem install bundle && bundle install",
    "dev": "(npm run apidocs:build && npm run synapse:build && npm run devcenter:dev) & npm run server:dev",
    "devcenter:build": "./build.sh",
    "devcenter:dev": "bundle exec jekyll serve",
    "devcenter:enterprise": "bundle exec jekyll serve --config _config.yml,_config-dev.yml,_config-enterprise.yml",
    "devcenter:public": "bundle exec jekyll serve --config _config.yml,_config-dev.yml",
    "setup": "npm run apidocs:setup && npm run synapse:setup && bundle install && npm install",
    "server:dev": "cross-env LOCAL=true npm run server:start",
    "server:start": "NODE_ENV=production node ./server/index.js",
    "start": "npm run apidocs:build && npm run synapse:build && npm run devcenter:build && npm run server:start",
    "synapse:build": "cd synapse && npm run build",
    "synapse:setup": "git submodule init && git submodule update && cd synapse && npm ci",
    "test": "cross-env NODE_ENV=production ./node_modules/.bin/mocha ./server/*.spec.js --timeout=10000"
  },
  "devDependencies": {
    "axios": "^0.19.0",
    "chai": "^4.2.0",
    "grunt": "~0.4.2",
    "grunt-contrib-imagemin": "~0.5.0",
    "grunt-contrib-jshint": "~0.8.0",
    "grunt-contrib-uglify": "~0.3.2",
    "grunt-contrib-watch": "~0.5.3",
    "grunt-imgcompress": "~0.1.1",
    "grunt-newer": "^0.7.0",
    "grunt-surround": "^0.1.0",
    "grunt-svgmin": "~0.3.1",
    "mocha": "^6.2.0",
    "shelljs": "^0.8.3"
  },
  "dependencies": {
    "bunyan": "^1.8.12",
    "cookie-parser": "^1.4.4",
    "cross-env": "^6.0.0",
    "express": "^4.17.1",
    "http-proxy-middleware": "^0.20.0"
  }
}
